import java.util.*;
public class MBA-Challange{
public static void main(String[] args){
Scanner sc = new scanner(System.in);
System.out.println("Input 1: ");
int n = sc.nextInt();
System.out.println("Input 2: ");
int k = sc.nextInt();
int s = (int)power(n,k);
int rem = s%10;
System.out.println(rem);
}
}
